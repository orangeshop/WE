<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
        android:id="@+id/tv_password_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/line_stroke_bottom_2_pink"
        android:padding="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_password_two"
        android:text=" "/>
    <TextView
        android:id="@+id/tv_password_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/line_stroke_bottom_2_pink"
        android:padding="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_password_one"
        app:layout_constraintEnd_toStartOf="@+id/tv_password_three"
        android:text=" "/>
    <TextView
        android:id="@+id/tv_password_three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/line_stroke_bottom_2_pink"
        android:padding="5dp"
        app:layout_constraintStart_toEndOf="@+id/tv_password_two"
        app:layout_constraintEnd_toStartOf="@+id/tv_password_four"
        app:layout_constraintTop_toTopOf="parent"
        android:text=" "/>
    <TextView
        android:id="@+id/tv_password_four"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/line_stroke_bottom_2_pink"
        android:padding="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_password_three"
        app:layout_constraintEnd_toStartOf="@+id/tv_password_five"
        android:text=" "/>
    <TextView
        android:id="@+id/tv_password_five"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/line_stroke_bottom_2_pink"
        android:padding="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_password_four"
        app:layout_constraintEnd_toStartOf="@+id/tv_password_six"
        android:text=" "/>
    <TextView
        android:id="@+id/tv_password_six"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/line_stroke_bottom_2_pink"
        android:padding="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_password_five"
        app:layout_constraintEnd_toEndOf="parent"
        android:text=" "/>
</androidx.constraintlayout.widget.ConstraintLayout>